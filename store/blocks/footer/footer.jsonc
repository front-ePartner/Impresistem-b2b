{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "props":{
      "blockClass": "footer--desktop"
    },
    "children": [
      "flex-layout.row#footer-1-desktop",
      "rich-text#medios--de--pago",
      "flex-layout.row#medios--de--pago",
      "flex-layout.row#footer-2-desktop"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "vtex.menu@2.x:menu#sobre--nosotros",
      "vtex.menu@2.x:menu#productos",
      "vtex.menu@2.x:menu#servicio--al--cliente",
      "vtex.menu@2.x:menu#contactanos",
      "flex-layout.col#siguenos--en"
    ],
    "props": {
      "blockClass": "menu-row",
      "paddingTop": 6,
      "paddingBottom": 3,
      "preventHorizontalStretch":true,
      "preventVerticalStretch":true
    }
  },
  "rich-text#medios--de--pago":{
    "props":{
      "blockClass": "medios--de--pago",
      "text":"medios de pago"
    }
  },
  "flex-layout.row#medios--de--pago":{
    "props":{
      "preventHorizontalStretch":true,
      "preventVerticalStretch":true
    },
    "children":[
      "image#payu",
      "image#visa",
      "image#mastercar"
    ]
  },
 
  "image#payu":{
    "props":{
      "src":"data:image/png;base64,",
      "maxHeight": 24
    }
  },
  "image#visa":{
    "props":{
      "blockClass":"logo--medios--de--pago--visa",
      "src":"data:image/png;base64,",
      "maxHeight": 24
    }
  },
  "image#mastercar":{
    "props":{
      "blockClass":"logo--medios--de--pago--mastercar",
      "src":"data:image/png;base64,",
      "maxHeight": 24
    }
  },
  "flex-layout.col#siguenos--en":{
    "props":{
      "preventHorizontalStretch":true,
      "preventVerticalStretch":true,
      "blockClass":"siguenos--en"
    },
    "children":[
      "rich-text#siguenos--en",
      "flex-layout.row#social-networks"
    ]
  },
  "rich-text#siguenos--en":{
    "props":{
      "blockClass": "siguenos--en",
      "text":"siguenos en"
    }
  },
  "flex-layout.row#social-networks":{
    "props":{
      "preventHorizontalStretch":true,
      "preventVerticalStretch":true
    },
    "children":[
      "flex-layout.col#facebook",
      "flex-layout.col#instagram"
    ]
  },
  "flex-layout.col#facebook":{
    "props":{
      "blockClass":"logo--siguenos--facebook"
    },
    "children":[
      "image#facebook"
    ]
  },
  "flex-layout.col#instagram":{
    "props":{
      "blockClass":"logo--siguenos--instagram"
    },
    "children":[
      "image#instagram"
    ]
  },
  "image#facebook":{
    "props":{
      "src":"data:image/png;base64,",
      "maxHeight": 24
    }
  },
  "image#instagram":{
    "props":{
      "blockClass":"logo--siguenos--instagram",
      "src":"data:image/png;base64,",
      "maxHeight": 24
    }
  },

  "flex-layout.row#footer-2-desktop": {
    "children": ["rich-text#footer"],
    "props": {
      "blockClass": "credits"
    }
  },
  "rich-text#footer": {
    "props": {
      "text": "Todos los derechos reservados",
      "blockClass": "footer"
    }
  },


  "footer-layout.mobile": {
    "props": {
      "blockClass": "footer--mobile"
    },
    "children": [
      "flex-layout.row#1-footer-mobile",
      "flex-layout.row#2-footer-mobile"
    ]
  },
  "flex-layout.row#1-footer-mobile": {
    "children":[
      "flex-layout.col#1-footer-mobile",
      "flex-layout.col#phone--redes--sociales",
      "flex-layout.col#medios--de--pago"
    ]
  },
  "flex-layout.col#1-footer-mobile":{
    "props":{
      "blockClass":"1-footer-mobile"
    },
    "children":[
      "vtex.menu@2.x:menu#phone"
    ]
  },
  "flex-layout.col#phone--redes--sociales":{
    "props":{
      "blockClass":"redes--sociales",
      "preventHorizontalStretch":true,
      "preventVerticalStretch":true
    },
    "children":[
      "image#phone--facebook",
      "image#phone--instagram"
    ]
  },
  "image#phone--facebook":{
    "props":{
      "blockClass": "phone--facebook",
      "src":"data:image/png;base64,",
      "maxHeight": 20
    }
  },
  "image#phone--instagram":{
    "props":{
      "blockClass": "phone--instagram",
      "src":"data:image/png;base64,",
      "maxHeight": 20
    }
  },
  "flex-layout.col#medios--de--pago":{
    "props":{
      "blockClass":"medios--de--pago"
    },
    "children":[
      "rich-text#phone--medios--de--pago",
      "image#phone--payu",
      "image#phone--visa"
    ]
  },
  "rich-text#phone--medios--de--pago":{
    "props":{
      "blockClass":"medios--de--pago--phone",
      "text":"medios de pago"
    }
  },
  "image#phone--payu":{
    "props":{
      "blockClass":"phone--payu",
      "src":"data:image/png;base64,",
      "maxHeight": 20
    }
  },
  "image#phone--visa":{
    "props":{
      "blockClass":"phone--visa",
      "src":"data:image/png;base64,",
      "maxHeight": 20
    }
  },
  "flex-layout.row#2-footer-mobile": {
    "children": ["flex-layout.col#footer-1-mobile"],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4
    }
  },
  "flex-layout.col#footer-1-mobile": {
    "children": [
      "rich-text#footer-mobile"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4,
      "rowGap": 4
    }
  },
  
  "rich-text#footer-mobile": {
    "props": {
      "text": "All stock and product photos are from photos.icons8.com",
      "blockClass": "footer"
    }
  }
}
